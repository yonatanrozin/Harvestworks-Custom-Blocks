{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "hw/program-category-artist",
  "title": "Program Category Artist",
  "icon": "user",
  "description": "Shows information about an artist (or group) belonging to the program category.",
  "style": "file:./style-index.css",
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": false,
        "padding": false
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "__experimentalFontFamily": true,
      "__experimentalTextDecoration": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "fitText": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    }
  },
  "attributes": {
    "postId": {
      "type": "number",
      "default": 0
    },
    "postType": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "usesContext": [
    "hw/artist-info"
  ],
  "providesContext": {
    "postId": "postId",
    "postType": "postTYpe"
  }
}