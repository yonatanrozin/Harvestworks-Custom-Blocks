{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "harvestworks/event-calendar",
  "version": "0.1.0",
  "title": "Event Calendar",
  "category": "widgets",
  "icon": "calendar",
  "description": "An interactive calendar that highlights days with scheduled events.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "has_event_color": {
      "type": "string",
      "default": "#a7ad8b"
    },
    "queried_day_color": {
      "type": "string",
      "default": "#c9cfad"
    }
  },
  "textdomain": "event-calendar",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}